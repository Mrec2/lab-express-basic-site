<body>
    <div class="d-flex flex-column justify-content-center align-items-center m-3">
        <div id="carouselExampleInterval" class="carousel slide h-50 w-50 " data-bs-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active" data-bs-interval="10">
                    <img src="/images/fast.webp" class="d-block w-100 carouselImgs" alt="fast">
                </div>
                <div class="carousel-item" data-bs-interval="10">
                    <img src="/images/vin2.png" class="d-block w-100 carouselImgs" alt="vin2">
                </div>
                <div class="carousel-item">
                    <img src="/images/vin.webp" class="d-block w-100 carouselImgs" alt="vin">
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleInterval"
                data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleInterval"
                data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>

        <div class="container ">
            <div class="row justify-content-around align-items-center">
                {{#each paths}}
                {{> cardpartial this}}
                {{/each}}
            </div>
        </div>
    </div>

</body>